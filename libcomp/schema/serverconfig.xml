<?xml version="1.0" encoding="UTF-8"?>
<objgen>
    <object name="ServerConfig">
        <member type="string" name="DiffieHellmanKeyPair"
            regex="^[a-fA-F0-9]{256}|$"/>
        <member type="u16" name="Port"/>
        <member type="string" name="DatabaseIP" default="127.0.0.1"/>
        <member type="string" name="CassandraKeyspace" default="comp_hack"/>
    </object>
</objgen>
